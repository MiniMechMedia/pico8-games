function drawKaiju(traits, cell)
	local spriteNumber = 4 * traits.bodyPlan + 64 * traits.pattern

	setColors(traits)
	local topLeft = 8 * cell
	spr(spriteNumber, topLeft.x, topLeft.y, 4, 4)
	pal()
end

