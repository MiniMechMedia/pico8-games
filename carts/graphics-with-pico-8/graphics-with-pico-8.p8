function count_elements(tbl)
	local string = ''
	local count = 0
	for el in all(tbl) do
		count += 1
		local myadd = tostr(el)
		if (myadd == '[table]') myadd = 't'
		-- if (myadd == '[table]') assert(false)
		string = myadd .. ',' .. string
	end
	return string
end

