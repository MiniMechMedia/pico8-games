pico-8 cartridge // http://www.pico-8.com
version 35
__lua__
--pascal-rorschach tweet         v0.1.0
--caterpillar games



cls()
poke(0x5f2c,6)
n=0
pset(63,0,7)
::_::
if (btnp(4)) printh(n,'@clip')
if btnp(5) then
	n+=1
	for x=0,127 do
		c=0
		for y=0,63 do
			a=pget(x,y)
			b=pget(x-1,y)
			pset(x,y+64,a^^b^^c)
			c=a&b|a&c|b&c
		end
	end
	memcpy(0x6000,0x7000+(n%4)\3,0x1000)
end
flip()
goto _
