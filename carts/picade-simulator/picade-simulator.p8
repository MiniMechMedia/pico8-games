      cy = 52
    },
    ['rightup'] = {
      sprite = 52,
      stick = 71,
      size = 2,
      size2=16,
      x = 33,
      y = 87
    },
    ['up'] = {
      sprite = 52,
      stick = 54,
      size = 2,
      size2=16,
      x = 30,
      y = 87,
      width = 17,
      height = 30,
      cx = 8,
      cy = 55
    },
    ['down'] = {
