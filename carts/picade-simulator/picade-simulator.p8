  if btn(0) then
    horiz = 'left'
  elseif btn(1) then
    horiz = 'right'
  end
  if btn(2) then
    vert = 'up'
  elseif btn(3) then
    vert = 'down'
  end

  local dir = horiz .. vert

  local ctrl = controls[dir]
  palt(11, true)
  palt(0, false)
  spr(ctrl.stick, 32+2, 96+5)
  sspr(32,40,17,17,ctrl.x,ctrl.y,
    ctrl.size2,ctrl.size2)
  local sprite = 132
  if btn(5) then
    sprite = 134
  end
  spr(sprite, 58, 109, 2,2)
  local bx,by = 32, 64
  if btn(4) then
    bx = 48
  end
  sspr(bx, by, 
    16, 16,
    60+1, 97
    ,15,16
    )
		poke(24365,1)
  local sprite = 136
