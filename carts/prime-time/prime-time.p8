            local vel = vec2(-tonum(btn(dirs.left)) + tonum(btn(dirs.right)),
                -tonum(btn(dirs.up)) + tonum(btn(dirs.down))
            )
            vel = vel:norm()
            self.pos += self.speed * gs.dt * vel
            self.pos.x = max(0, min(self.pos.x, 128))
            self.pos.y = max(0, min(self.pos.y, 128))
