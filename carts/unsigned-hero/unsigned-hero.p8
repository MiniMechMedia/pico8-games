version 32
__lua__
--unsigned hero                  v0.2.0
--caterpillar games




function hasAnimation() 
	return gs.currentAnimation != nil and
		costatus(gs.currentAnimation) != 'dead'
end

function getLevel()
	return peek(0x4300)
end

function setLevel(value)
	return poke(0x4300, value)
end

function _init()
	music(27, 2000)
	gs = {
		currentAnimation = nil,
		dt = 1/30,
		-- level = 1,
		stairsAreOpen = false,
		stairsOpen = function(self)
			return self.stairsAreOpen
		end,
		potions = {},
		stairs = {
			pos = vec2(64, 64)
		},
		player = {
			pos = vec2(64, 64),
			spriteNumber = 4,
			isAttacking = false,
			drawAttackCounter = 0,
			shouldDrawAttack = function(self)
			-- assert(false)
				return self.drawAttackCounter > 0
			end,
			isBlocking = false,
			getSpeed = function(self)
				if self.isBlocking then
					return self.baseSpeed / 2
				end
				return self.baseSpeed
			end,
			swordReach = 10,
			attack = 100,
			baseSpeed = 40,
			health = 100,
			maxHealth = 100,
			facing = dirs.down,
			-- How long before you can take damage right now
			damageCountdown = 0,
			-- After getting hit, how long before you can take damage again
			damageCooldown = 6
		},
		enemies = {
			makeEnemy(40, 40)
		}
	}

	setLevel(1)
end

function makePotion(pos)
	add(gs.potions, {
		pos = pos,
		healthBoost = 30,
		spriteNumber = 1
	})
end	

function makeEnemy(x, y)
	return {
		pos = vec2(x, y),
		baseSpeed = 15,
		getSpeed = function(self)
			return self.baseSpeed
		end,
		attackDamage = 10,
		attackCooldown = 40,
		attackCountdown = 0,
		spriteNumber = 8,
		facing = dirs.left,
		aiMethod = zombieAi,
		isDead = false,
		health = 100
	}
end

function tryAttack(enemy)
	if enemy.attackCountdown <= 0 then
		-- TODO animate
		inflictDamage(enemy.attackDamage)
		enemy.attackCountdown = enemy.attackCooldown
	end
end

function zombieAi(self)
	local diff = gs.player.pos - self.pos
	
	if diff:mag() < 5 then
		tryAttack(self)
	end

	local shouldMove = diff:mag() > 2

	diff = diff:norm() 
	diff *= self.baseSpeed
	if shouldMove then
		self.pos += diff * gs.dt
	end
end

function vecFromAngle(angle)
	return vec2(cos(angle), sin(angle))
end

vecMeta = {
	__add = function(v1, v2) 
		return vec2(v1.x + v2.x, v1.y + v2.y)
	end,
	__sub = function(v1, v2)
		return vec2(v1.x - v2.x, v1.y - v2.y)
	end,
	__mul = function(s, v)
		if type(s) == 'table' then
			s,v = v,s
		end
		return vec2(s * v.x, s * v.y)
	end,
	__div = function(v, s)
		return vec2(v.x / s, v.y / s)
	end
}


function vec2(x, y)
	local ret = {
		x = x,
		y = y,
		mag = function(self)
			return sqrt(self.x * self.x + self.y * self.y)
		end,
		norm = function(self)
			if abs(self.x) < 0.001 and abs(self.y) < 0.001 then
				return vec2(0, 0)
			end
			return self / self:mag()
		end,
		ang = function(self)
			return atan2(self.x, self.y)
		end
	}

	setmetatable(ret, vecMeta)
	return ret
end

dirs = {
	left = 0,
	right = 1,
	up = 2,
	down = 3,
	z = 4,
	x = 5
}

function dirToVec(dir)
	if dir == dirs.left then
		return vec2(-1, 0)
	elseif dir == dirs.right then
		return vec2(1, 0)
	elseif dir == dirs.up then
		return vec2(0, -1)
	elseif dir == dirs.down then
		return vec2(0, 1)
	end
end

function acceptInput()
	local vel = vec2(0, 0)
	local facing = nil
	if btn(dirs.left) then
		vel.x -= 1
		facing = dirs.left
	end
	if btn(dirs.right) then
		vel.x += 1
		facing = dirs.right
	end
	if btn(dirs.up) then
		vel.y -= 1
		facing = dirs.up
	end
	if btn(dirs.down) then
		vel.y += 1
		facing = dirs.down
	end

	if btnp(dirs.x) then
		gs.player.isAttacking = true
	end

	-- -- TODO comment out
	-- -- gs.player.isBlocking = btn(dirs.z)
	-- if btnp(dirs.z) then
	-- 	gs.enemies = {}
	-- 	-- gs.currentAnimation = cocreate(function()
	-- 	-- gs.player.health = 0
	-- 	-- end)
	-- end


	vel = vel:norm()

	gs.player.facing = facing or gs.player.facing
	gs.player.pos += vel * gs.player:getSpeed() * gs.dt

	gs.player.pos.x = mid(12, gs.player.pos.x, 116)
	gs.player.pos.y = mid(12, gs.player.pos.y, 107)

end

function mymget(vec)
	if vec == nil then
		vec = gs.player.pos
	end
	local cell = vec / 8 
	return mget(cell.x, cell.y)
end

function inflictDamage(amount)
	if gs.player.damageCountdown <= 0 then
		gs.player.health -= amount
		gs.player.damageCountdown = gs.player.damageCooldown
	end
end

function checkSpikes()
	local spikeDamage = 10
	if fget(mymget()) & 1 > 0 then
		inflictDamage(spikeDamage)
	end
end

function updateEnemies()
	for enemy in all(gs.enemies) do 
		enemy:aiMethod()
	end
end

drawAttackDuration = 3

function hitEnemy(enemy)
	-- TODO add knockback
	-- TODO this as well
	enemy.health -= gs.player.attack
	if enemy.health <= 0 then
		enemy.isDead = true	
		-- if rnd() < 0.25 then
		-- -- if true then
		-- 	makePotion(enemy.pos)
		-- end	
	end
end

function executePlayerAttack()
	if gs.player.isAttacking then
		gs.player.isAttacking = false
		gs.player.drawAttackCounter = drawAttackDuration
		
		local angles = attackAngles()

		for enemy in all(gs.enemies) do
			local delta = enemy.pos - gs.player.pos
			if delta:mag() <= gs.player.swordReach + 3 then
				local ang1 = delta:ang()
				local ang2 = delta:ang() - 1
				-- Now check if within angle
				if (angles.min <= ang1 and ang1 <= angles.max) or
					(angles.min <= ang2 and ang2 <= angles.max)
					-- or delta:mag() < 2 
					then
					-- We have a hit
					hitEnemy(enemy)
				end
			end			
		end
	end
end

function checkLevel255()
	if gs.win then return end
	if getLevel() == 255 then
		gs.win = true
	end
end

function _update()
	if gs.win then
		if btnp(dirs.x) then
			_init()
			return
		end
	end
	if hasAnimation() then
		return
	end

	checkLevel255()
	acceptInput()

	executePlayerAttack()

	updateEnemies()

	checkSpikes()

	coolDowns()

	cleanUpEnemies()

	checkStairs()
	checkPotions()

	checkDeath()

end

function checkPotions()
	for pot in all(gs.potions) do
		local diff = pot.pos - gs.player.pos
		if diff:mag() < 4 then
			gs.player.health = min(gs.player.maxHealth, gs.player.health + pot.healthBoost)
			del(gs.potions, pot)
		end
	end
end

function checkDeath()
	if gs.player.health <= 0 then
		gs.currentAnimation = cocreate(function()
			-- TODO death animation
			for i = 1, 30 do
				local i = 25
				local yoff = -10

				drawLevelText(18 * 2 - 12, i * 2 + yoff, 'dead')
				yield()
			end

			local digits = parseVal(getLevel())
			local xoff = 36
			if #digits < 2 then
				xoff = 50
			end
			local otherDigits = {}
			add(otherDigits, 72)
			add(otherDigits, 130)	-- -1
			for i = 30, 0, -1 do
				drawText(digits, xoff, 64 - 20)
				drawText(otherDigits, xoff + 18 * #digits + i * 2, 64 - 20)
				yield()
			end	

			for i = 5, 0, -1 do
				drawText(digits, xoff, 64 - 20)
				drawText(otherDigits, xoff + 18 * #digits, 64 - 20)
				yield()
			end	
			gs.player.health = gs.player.maxHealth
			gs.player.pos = vec2(50, 64)
			setLevel(getLevel() - 1)

			digits = parseVal(getLevel())

			gs.enemies = {}
			spawnEnemies(getLevel())

			for i = 1, 30 do
				drawText(digits, 50, 64 - 20)
				yield()
			end	

		end)

	end
end

function rndrange(min, max)
	local delta = max - min
	return min + rnd(delta)
end

function getEnemyLocation()
	local ret = vecFromAngle(rnd()) * rndrange(30, 50) + vec2(64, 64)
	while (fget(mymget()) & 1) > 0 do
		ret = vecFromAngle(rnd()) * rndrange(30, 50) + vec2(64, 64)
	end
	return ret
end

function spawnEnemies(number)
	gs.potions = {}
	for i = 1, number do
		local location = getEnemyLocation()
		add(gs.enemies, makeEnemy(location.x, location.y
			))
	end
end

function levelUp()
	if getLevel() == 254 then
		gs.natWin = true
	end
	setLevel(getLevel() + 1)
	gs.stairsAreOpen = false
	spawnEnemies(getLevel())
end

function yieldfor(frames)
	for i = 1, frames do
		yield()
	end
end

function drawText(words, x, y)
	palt(14, true)
	palt(0, false)
	for i = 1, #words do
		spr(words[i], x + (i - 1) * 18, y, 2, 2)
	end
	palt()
end

function drawLevelText(x, y, which)
	local L = 64
	local E = 66
	local V = 68
	local C = 96
	local A = 98
	local R = 100
	local D = 102

	local words = {
		L, E, V, E, L
	}

	if which == 'dead' then
		words = {
			D, E, A, D
		}
	elseif which then
		words = {
			 C, L, E, A, R --, E, D
		}
	end

	drawText(words, x, y)
end

function parseVal(num)
	local digits = {
		130,
		132,
		134,
		136,
		138,
		140,
		142,
		160,
		162
	}
	digits[0] = 128

	local ret = {}
	if num == 0 then
		return {digits[0]}
	end
	while num > 0 do
		lastDigit = num % 10
		num = flr((num - lastDigit) / 10)
		add(ret, digits[lastDigit])
	end

	local ret2 = {}
	for i = #ret, 1, -1 do
		add(ret2, ret[i])
	end
	return ret2
end

function checkStairs()
	if not gs.stairsAreOpen and #gs.enemies == 0 then
		gs.currentAnimation = cocreate(function()
			local yoff = -10
			for i = 1, 25 do
				drawLevelText(18 * 2 - 12, i * 2 + yoff, false)
				drawLevelText(18 * 2 - 12, 128 - i*2 + yoff, true)
				yield()
			end
			for j = 1, 10 do
				local i = 25
				drawLevelText(18 * 2 - 12, i * 2 + yoff, false)
				drawLevelText(18 * 2 - 12, 128 - i*2 + yoff, true)
				yield()
			end
			gs.stairsAreOpen = true
			local loc = getEnemyLocation()
			makePotion(loc)
		end)
		return
	end
	local diff = gs.player.pos - gs.stairs.pos
	if diff:mag() < 3 and gs:stairsOpen() then
		gs.currentAnimation = cocreate(function()
			local digits = parseVal(getLevel())
			local otherDigits = {}
			local xoff = 36
			if #digits < 2 then
				xoff = 50
			end
			add(otherDigits, 70)
			add(otherDigits, 130)	-- +1
			for i = 30, 0, -1 do
				drawText(digits, xoff, 64 - 20)
				drawText(otherDigits, xoff + 18 * #digits + i * 2, 64 - 20)
				yield()
			end	
			
			for i = 5, 0, -1 do
				drawText(digits, xoff, 64 - 20)
				drawText(otherDigits, xoff + 18 * #digits, 64 - 20)
				yield()
			end	

			levelUp()
			digits = parseVal(getLevel())


			for i = 1, 30 do
				drawText(digits, xoff, 64 - 20)
				yield()
			end	
		end)
	end
end

function cleanUpEnemies()
	for enemy in all(gs.enemies) do
		if enemy.isDead then
			del(gs.enemies, enemy)
		end
	end
end

function coolDowns()
	if gs.player.damageCountdown > 0 then
		gs.player.damageCountdown -= 1
	end

	for enemy in all(gs.enemies) do
		enemy.attackCountdown = max(0, enemy.attackCountdown - 1)
	end
end


function isInBounds(v)
	return 10 < v.x and v.x < 120
		and 10 < v.y and v.y < 110
end

function drawBackground()
	map(0, 0, 0, 0, 16, 16)
end

function drawAttack()
	if gs.player:shouldDrawAttack() then
		gs.player.drawAttackCounter -= 1
		-- local angles = attackAngles()
		local angle = dirToVec(gs.player.facing):ang()
		local angleMin = angle - 1/8 + 
			gs.player.drawAttackCounter / drawAttackDuration * 1/8
		local angleMax = angle + 1/8 + 
			gs.player.drawAttackCounter / drawAttackDuration * 1/8

		for radius = 5, gs.player.swordReach, 0.3 do
			for ang = angleMin, angleMax, 0.0183 do
				local loc = gs.player.pos + radius * vecFromAngle(ang)
				pset(loc.x, loc.y, 7)
				-- pset()
			end
		end
	end
end

function drawPlayer()
	if gs.player.damageCountdown <= 0 or
		(gs.player.damageCountdown % 3 != 0) or hasAnimation()
		then

		local drawBodyFirst = true
		if gs.player.isBlocking and gs.player.facing == dirs.up then
			drawBodyFirst = false
		end


		if drawBodyFirst then
			drawCharacter(gs.player)
		end

		if gs.player.isBlocking then
			spr(20, gs.player.pos.x - 1, gs.player.pos.y - 1)
		else
			if gs.player:shouldDrawAttack() then
				drawAttack()
			else
				spr(21, gs.player.pos.x - 1, gs.player.pos.y - 1)
			end
		end

		if  not drawBodyFirst then
			drawCharacter(gs.player)
		end
		
	end
end

function drawCharacter(character)
	palt(0, false)
	palt(14, true)
	spr(character.spriteNumber + character.facing,
		character.pos.x - 4, character.pos.y - 4)
	palt()
end

function drawEnemies()
	for enemy in all(gs.enemies) do
		-- assert(false)
		drawCharacter(enemy)
	end
end

function drawHealthBar()
	local xoff = 30
	local yoff = 115
	rect(xoff - 1, yoff - 1, 128 - xoff + 1, yoff + 10 + 1, 2)
	rectfill(xoff, yoff, 128 - xoff, yoff + 10, 0)
	local maxLength = 128 - 2 * xoff
	local healthLength = max(gs.player.health,0) / gs.player.maxHealth * maxLength
	rectfill(xoff, yoff, xoff + healthLength, yoff + 10, 8)
end

function attackAngles()
	local angle = dirToVec(gs.player.facing):ang()
	return {
		min = angle - 1/8,
		max = angle + 1/8
	}
end

function drawStaircase()
	if gs:stairsOpen() then
		spr(46, gs.stairs.pos.x - 4, gs.stairs.pos.y - 4)
	end		
end

function drawWin()
	print('')
	print(' you made it to level 255!', 7)
	print('')
	print(' you escaped the dungeon', 7)
	print(' and lived happily ever after', 7)
	print('')
	print('')
	print(' press ❎ to play again', 7)
	print('')
	print('')
	print('')
	if gs.natWin then
		print(' (woah you really fought')
		print(' through 255 levels.')
		print(' there is an easier way,')
		print(' but congratulations!!!)')
	end
end

function drawPotions()
	for pot in all(gs.potions) do
		spr(pot.spriteNumber, pot.pos.x-2, pot.pos.y-2)
	end
end

function _draw()
	cls(0)
	if gs.win then
		drawWin()
		return
	end
	drawBackground()

	drawPotions()

	drawEnemies()

	drawStaircase()

	drawPlayer()

	drawHealthBar()

	if hasAnimation() then
		local active, exception = coresume(gs.currentAnimation)
		if exception then
			stop(trace(gs.currentAnimation, exception))
		end
	end
	-- print(mymget())
end


