version 35
__lua__
--skyline tweet                  v0.1.0
--caterpillar games


y=rnd(128)
h=128
cls(1)
::_::
memcpy(0x6001,0x6000,0x2000)
if rnd() < 0.1 then
	y = mid(y + rnd(60) - 30, 0, h)
end
rectfill(0,0,1,h,1)
rectfill(0,y,1,h,0)
for w = y+2, h, 2 do
	if rnd() < (h-y)/600 then
		pset(x,w,7)
	end
end
if rnd() < 0.01 then
	y = rnd(h)
end
flip()
goto _

